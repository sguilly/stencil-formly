{
  "name": "stencil-formly",
  "version": "0.0.4",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-formly/stencil-formly.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "examples": "stencil build && http-server -o /examples",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "3.2.0",
    "@stencil/store": "2.0.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.0-rc.10",
    "@storybook/addon-links": "7.0.0-rc.10",
    "@storybook/blocks": "7.0.0-rc.10",
    "@storybook/testing-library": "0.0.13",
    "@storybook/web-components": "7.0.0-rc.10",
    "@storybook/web-components-webpack5": "7.0.0-rc.10",
    "lit": "2.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.0-rc.10"
  },
  "license": "MIT"
}
